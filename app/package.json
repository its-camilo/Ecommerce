{
  "name": "ecommerce",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "homepage": "https://its-camilo.github.io/Ecommerce",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --public-url https://its-camilo.github.io/Ecommerce && node ./scripts/fix-favicon.js",
    "serve:web": "npx serve dist -s",
    "deploy": "npm run build:web && npx gh-pages -d dist",
    "test": "jest --watchAll",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "expo": "~52.0.46",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.7.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "^0.76.9",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-paper": "^5.13.2",
    "react-native-root-toast": "^4.0.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eas-cli": "^16.17.4",
    "gh-pages": "^6.1.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "prettier": "^3.5.3",
    "react-test-renderer": "18.3.1",
    "serve": "^14.2.4",
    "typescript": "^5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "jwt-decode",
          "react-native-keyboard-aware-scrollview",
          "react-native-snap-carousel",
          "react-native-vector-icons"
        ]
      }
    }
  },
  "private": true
}
